{
    "name": "buzzingpixel/craft-static",
    "version": "1.0.4",
    "type": "craft-plugin",
    "description": "Static file caching for Craft 3",
    "license": "Apache-2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-static"
    ],
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "buzzingpixel\\craftstatic\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "extra": {
        "name": "Craft Static",
        "handle": "craft-static",
        "developer": "TJ Draper",
        "developerUrl": "https://buzzingpixel.com",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "buzzingpixel\\craftstatic\\Craftstatic"
    }
}
