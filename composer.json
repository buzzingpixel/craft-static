{
    "name": "buzzingpixel/craft-static",
    "description": "Static file caching for Craft 3",
    "version": "2.0.2",
    "type": "craft-plugin",
    "license": "Apache-2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-static"
    ],
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "craftcms/cms": "^3.1.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "filp/whoops": "^2.4",
        "friendsofphp/php-cs-fixer": "^2.15",
        "lucidtaz/yii2-whoops": "dev-master",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11.12",
        "phpstan/phpstan-deprecation-rules": "^0.11.2",
        "phpunit/phpunit": "^8.2",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\craftstatic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "buzzingpixel\\tests\\": "tests",
            "buzzingpixel\\testenvironment\\": "testenvironment/src"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true
    },
    "extra": {
        "name": "Craft Static",
        "handle": "craft-static",
        "developer": "TJ Draper",
        "developerUrl": "https://buzzingpixel.com",
        "schemaVersion": "2.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "buzzingpixel\\craftstatic\\Craftstatic"
    }
}
